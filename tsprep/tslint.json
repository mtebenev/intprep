{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
      "arrow-parens": false,
      "max-classes-per-file": false,
      "member-ordering": false,
      "no-bitwise": false,
      "ordered-imports": false,
      "prefer-for-of": false,
      "quotemark": [true, "single"],
      "trailing-comma": false,
      "whitespace": false
    },
    "rulesDirectory": []
}
